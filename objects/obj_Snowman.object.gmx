            <string>///Movement

var rkey = keyboard_check(vk_right);
var lkey = keyboard_check(vk_left);
var jkey = keyboard_check_pressed(vk_up);

if (place_meeting(x, y + 1, obj_Snow_Floor)) {

    jumps = 2;

}

if (vspd &lt; 10) {  
    vspd += grav;   
}
 
if ((jkey) &amp;&amp; (jumps &gt; 0)) {

    vspd = -jspd;
    jumps -= 1;
    
}

//Right or Left
if (rkey) { 
    hspd = spd; 
    sprite_index = spr_Snowman_Walk;
}
if (lkey) { 
    hspd = -spd; 
    sprite_index = spr_Snowman_Walk_Bw;
}

//Not moving
if ((!rkey &amp;&amp; !lkey) || (rkey &amp;&amp; lkey)) {
    hspd = 0;
    sprite_index = spr_Snowman;
}

scr_Collisions();

//Ladder Climbing
if (place_meeting(x, y, obj_Ladder) &amp;&amp; (keyboard_check(vk_up))) {
    vspd -= 0.8;
}


</string>
