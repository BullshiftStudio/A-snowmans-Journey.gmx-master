            <kind>1</kind>
            <string>///Change the Image and Setting

if (image_index = 1) { //if turned on
    
    image_index = 0;
    global.fullscreen = false;
    
} else if (image_index = 0) {

    image_index = 1;
    global.fullscreen = true;
    
}

ini_open("user_options.ini");
ini_write_real("Checkboxes", "fullscreen", global.fullscreen);
ini_write_real("Checkboxes", "fullscreen", image_index);
ini_close();

if (global.fullscreen = true) {

    ///Apply Resolution

    width = display_get_width();
    height = display_get_height();

    //Set the Resolution
    view_wview = width;
    view_hview = height;
    view_wport[0] = view_wview;
    view_hport[0] = view_hview;

    //Resize Application Surface
    surface_resize(application_surface, view_wview, view_hview);
    window_set_size(view_wview, view_hview);
    display_set_gui_size(view_wview, view_hview);

    window_set_fullscreen(true);

} else {

    width = 1280;
    height = 720;

    window_set_size(width, height);
    view_wview = width;
    view_hview = height;
    view_wport[0] = view_wview;
    view_hport[0] = view_hview;
    
    surface_resize(application_surface, view_wview, view_hview);
    window_set_size(view_wview, view_hview);
    display_set_gui_size(view_wview, view_hview);

    window_set_fullscreen(false);

}
    

</string>
